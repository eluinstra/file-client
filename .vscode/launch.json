  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch Start",
      "request": "launch",
      "mainClass": "dev.luin.file.client.Start",
      "projectName": "file-client",
      "args": ["-hsqldb", "-port", "8000"],
      "vmArgs": ["-Djavax.net.ssl.trustStore=", "-Dlog4j.configurationFile=log4j2.xml"]
    },
    {
      "type": "java",
      "name": "Launch StartGB",
      "request": "launch",
      "mainClass": "dev.luin.file.client.StartGB",
      "projectName": "file-client",
      "args": ["-hsqldb", "-port", "8000"],
      "vmArgs": ["-Djavax.net.ssl.trustStore=", "-Dlog4j.configurationFile=log4j2.xml"]
    }
  ]
}